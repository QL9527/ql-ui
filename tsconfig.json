{
  "compilerOptions": {
    "baseUrl": "./",
    "target": "ESNext",
    "module": "CommonJS",
    "jsx": "react",
    "declaration": true,
    "declarationDir": "lib",
    "strict": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "skipLibCheck": true
  },
  "include": [
    "src",
    ".dumi/**/*",
    ".dumirc.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
